												<th><input type="text" class="form-control column-filter" placeholder=" Əməliyyatlar" disabled></th>
											</tr>
										</thead>
										<tbody>
											@foreach (var item in Model)
											{
												<tr>
													<td>@item.ReservationDate.ToString("dd.MM.YYYY")</td>
													<td>@item.Customers.FullName</td>
													<td>@item.Master.FullName</td>
													<td>
														@foreach (var reports in item.LazerAppointmentReports)
														{
															<div>@reports.LazerCategory.Name</div>
														}
													</td>
													<td>
														@if (item.IsStarted)
